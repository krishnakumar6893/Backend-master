<article id="scroller">
  <ul class="network">
    <% @updates_by_user.each do |uid, feeds| %>
      <% feeds = feeds.group_by(&:klass_sym) %>
      <% feeds.each do |klass, farr| %>
        <%= render ('network_' + klass.to_s + '_content'), :uid => uid, :feeds => farr %>
      <% end %>
    <% end %>
    <% if @updates_by_user.empty? %>
      <li><%= @current_user.friend_ids.empty? ? 'You are not following anyone' : 'No updates from your network' %></li>
    <% end %>
  </ul>
</article>
