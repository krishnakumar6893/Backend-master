<article id="scroller">
  <ul class="updates">
    <%= render 'updates_all' %>
  </ul>
  <div class='loader'>
    <%= render 'updates_load_more' %>
  </div>
</article>

<script type="text/javascript">
  jQuery(function($) {
    $("#load_more").live("ajax:before", function() {
      $(this).html('Loading...');
    });
    $("#load_more").live("ajax:error", function() {
      $(this).html('Failed! Try again.');
    });
  });
</script>
